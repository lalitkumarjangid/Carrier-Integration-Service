# ─── UPS API Credentials ───────────────────────────────────────────────
# UPS OAuth 2.0 client credentials (from UPS Developer Portal)
UPS_CLIENT_ID=your_ups_client_id
UPS_CLIENT_SECRET=your_ups_client_secret

# UPS Shipper account number
UPS_ACCOUNT_NUMBER=your_ups_account_number

# ─── UPS API Configuration ────────────────────────────────────────────
# Base URL for UPS API (production: https://onlinetools.ups.com, testing: https://wwwcie.ups.com)
UPS_BASE_URL=https://onlinetools.ups.com

# Rating API version
UPS_RATING_API_VERSION=v2409

# ─── General Settings ─────────────────────────────────────────────────
# Request timeout in milliseconds
HTTP_TIMEOUT=10000

# Log level (debug, info, warn, error)
LOG_LEVEL=info

# Application identifier for UPS transaction source header
TRANSACTION_SOURCE=CybershipCIS

# Node environment
NODE_ENV=development
